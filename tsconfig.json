{
  "compilerOptions": {
    "allowSyntheticDefaultImports": true,
    "esModuleInterop": true,
    "jsx": "react",
    "lib": [
      "dom",
      "es2019"
    ],
    "module": "ESNext",
    "allowJs": true,
    "moduleResolution": "node",
    "noImplicitReturns": true,
    "noUnusedLocals": true,
    "strict": true,
    "target": "es2018",
    "baseUrl": "src",
    "paths": {
      "@/*": [
        "./*"
      ]
    }
  }
}